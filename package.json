{
  "name": "peerio-s3",
  "version": "1.0.4",
  "description": "Knox wrapper",
  "main": "index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeerioTechnologies/peerio-s3.git"
  },
  "keywords": [
    "ceph",
    "s3",
    "knox",
    "s3-compatible",
    "riak-cs",
    "swift",
    "aws-s3",
    "aws"
  ],
  "author": {
    "name": "Samuel Martin Moro",
    "email": "samuel@peerio.com",
    "url": "https://peerio.com"
  },
  "contributor": [
    {
      "name": "Vitaly Aminev",
      "email": "v@aminev.me",
      "web": "https://makeomatic.ru/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-s3/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-s3",
  "dependencies": {
    "backoff": "^2.5.0",
    "bluebird": "^3.4.7",
    "debug": "latest",
    "myknox": "^0.9.3",
    "lodash": "^4.17.3"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "husky": "^1.3.1",
    "semantic-release": "^15.13.3",
    "validate-commit-msg": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "WIP"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+"
    }
  }
}
